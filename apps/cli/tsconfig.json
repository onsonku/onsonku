{
  "extends":["@ossonu/tsconfig/base.json"],
  "display": "Node LTS",
  "_version": "18.1.0",
  "compilerOptions": {
    "lib": [
      "es2023"
    ],
    "module": "NodeNext",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "noUncheckedIndexedAccess": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "NodeNext"
  },
  "include": [
    "src"
, "tsup.config.ts"  ]
}